<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="TOP_CENTER" focusTraversable="true" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="550.0" style="-fx-border-color: BLACK;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SudokuController">
   <children>
      <GridPane fx:id="gridPane" focusTraversable="true" prefHeight="550.0" prefWidth="550.0">
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
      </GridPane>
      <HBox alignment="TOP_CENTER" focusTraversable="true" prefHeight="50.0" prefWidth="550.0" spacing="12.0">
         <children>
            <Button fx:id="setButton" mnemonicParsing="false" onAction="#setBoard" text="set" />
            <Button fx:id="clearButton" mnemonicParsing="false" onAction="#clearBoard" text="clear" />
         </children>
         <padding>
            <Insets top="13.0" />
         </padding>
      </HBox>
   </children>
</VBox>
